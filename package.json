{
  "name": "gitlab-easy-time-tracking",
  "version": "0.2.1",
  "private": true,
  "description": "A browser plugin for tracking time for GitLab merge-requests",
  "scripts": {
    "build-prod": "NODE_ENV=production node utils/build.js",
    "build": "node utils/build.js",
    "start": "node utils/webserver.js",
    "format": "prettier --write 'src/css/*.css' 'src/**/*.js'",
    "test": "jshint src & prettier --check 'src/css/*.css' 'src/**/*.js'"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jshint": "^2.10.2",
    "prettier": "1.18.2",
    "style-loader": "^0.23.1",
    "webextension-polyfill": "^0.4.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "3.1.14",
    "write-file-webpack-plugin": "4.5.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "easytimer.js": "^3.2.0",
    "imports-loader": "^0.8.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "jshintConfig": {
    "esversion": 9,
    "-W138": true
  }
}
